{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "app-ui": {
      "root": "libs/app-ui",
      "sourceRoot": "libs/app-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/app-ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/app-ui"
          ],
          "options": {
            "jestConfig": "libs/app-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "website": {
      "root": "apps/website",
      "sourceRoot": "apps/website",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/website",
            "outputPath": "dist/apps/website"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "website:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "website:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "website:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/website"
          ],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/website/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "website-e2e": {
      "root": "apps/website-e2e",
      "sourceRoot": "apps/website-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/website-e2e/cypress.json",
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "devServerTarget": "website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "website:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/website-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ide": {
      "root": "apps/ide",
      "sourceRoot": "apps/ide/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "apps/ide/public",
            "deleteOutputPath": false,
            "index": "apps/ide/resources/views/app.blade.php",
            "main": "apps/ide/src/main.tsx",
            "polyfills": "apps/ide/src/polyfills.ts",
            "tsConfig": "apps/ide/tsconfig.app.json",
            "assets": [
              "apps/ide/src/favicon.ico",
              "apps/ide/src/assets"
            ],
            "styles": [
              "apps/ide/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ide/src/environments/environment.ts",
                  "with": "apps/ide/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ide:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "ide:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ide/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ide"
          ],
          "options": {
            "jestConfig": "apps/ide/jest.config.js",
            "passWithNoTests": true
          }
        },
        "fix-build": {
          "executor": "./tools/executors/acide:fix-build",
          "options": {
            "outputPath": "apps/ide/public",
            "replaceFiles": [
              {
                "src": "apps/ide/public/app.blade.php",
                "dest": "apps/ide/resources/app.blade.php"
              }
            ]
          }
        }
      }
    },
    "ide-e2e": {
      "root": "apps/ide-e2e",
      "sourceRoot": "apps/ide-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ide-e2e/cypress.json",
            "tsConfig": "apps/ide-e2e/tsconfig.e2e.json",
            "devServerTarget": "ide:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ide:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ide-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}
