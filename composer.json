{
  "name": "acide/monorepo",
  "description": "ACID-E - Smart PHP IDE",
  "type": "project",
  "keywords": [
    "php",
    "ide",
    "vscode",
    "acide",
    "acid-e",
    "typescript",
    "react",
    "editor",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "beyondcode/laravel-websockets": "^1.11",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.12",
    "laravel/tinker": "^2.5",
    "php": "^7.3|^8.0",
    "pusher/pusher-php-server": "~3.0"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "apps/ide/app/",
      "Database\\Factories\\": "apps/ide/database/factories/",
      "Database\\Seeders\\": "apps/ide/database/seeders/"
    },
    "classmap": [
      "tools/console/commands"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "apps/ide/tests/"
    }
  },
  "extra": {
    "laravel": []
  },
  "replace": {
    "acide/acide": "self.version"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
